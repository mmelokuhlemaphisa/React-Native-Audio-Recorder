{
  "expo": {
    "name": "React-Native-Audio-Recorder",
    "slug": "React-Native-Audio-Recorder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/AudioImage.png",
    "scheme": "reactnativeaudiorecorder",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/AudioImage.png",
        "backgroundImage": "./assets/images/AudioImage.png",
        "monochromeImage": "./assets/images/AudioImage.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.melokuhle1.ReactNativeAudioRecorder"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/AudioImage.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/AudioImage.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4d78b9af-9e9d-4c66-b70d-a9b385e92533"
      }
    }
  }
}
